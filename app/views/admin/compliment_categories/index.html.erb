<div class="row">
	<h3 class="col-xs-12 col-sm-6"><%= @controller_name %> <%= actionNameChange controller.action_name %></h3>
	<div class="col-xs-12 col-sm-6">

	</div>
</div>
<div class="table-responsive">
  <table width="100%" cellpadding="0" cellspacing="0" class="table table-bordered table-striped">
		<colgroup>
      <col style="width:100px" />
      <col />
      <col style="width:130px" />
      <col style="width:100px" />
      <col style="width:130px" />
      <% if can?(:update, ComplimentCategory) || can?(:delete, ComplimentCategory) %>
      <col <%=raw manage_width(ComplimentCategory) %> />
      <% end %>
    </colgroup>
		<thead>
			<tr>
				<th><%= t(:label_number) %></th>
				<th><%=t(:title,scope:[:activerecord,:attributes,:compliment_category]) %></th>
				<th><%=t(:compliments_count,scope:[:activerecord,:attributes,:compliment_category]) %></th>
				<th><%=t(:enable,scope:[:activerecord,:attributes,:compliment_category]) %></th>
				<th><%= t(:label_created_at) %></th>
				<% if can?(:update, ComplimentCategory) || can?(:delete, ComplimentCategory) %>
				<th><%= t(:label_manage) %></th>
				<% end %>
			</tr>
		</thead>
		<tbody>
			<% unless @admin_compliment_categories.empty? %>
			<% @admin_compliment_categories.each do |compliment_category| %>
			<tr>
				<td><%= link_to compliment_category.id,admin_compliment_category_path(compliment_category) %></td>
				<td><%= link_to compliment_category.title,admin_compliment_category_path(compliment_category) %></td>
				<td><%= link_to compliment_category.compliments_count,admin_compliment_category_path(compliment_category) %></td>
				<td><%= raw flagToString(compliment_category.enable) %></td>
				<td><%= compliment_category.created_at.to_date %></td>
				<% if can?(:update, compliment_category) || can?(:delete, compliment_category) %>
				<td> <% if can? :update, compliment_category %>
				<%= link_to edit_admin_compliment_category_path(compliment_category),:class=>"btn btn-default" do %> <i class="glyphicon glyphicon-pencil"></i> <%= t(:link_edit) %>
				<% end %>
				<% end %>
				<% if can? :delete, compliment_category %>
				<%= link_to admin_compliment_category_path(compliment_category), :method => :delete, :data => { :confirm => t(:delete_confirm) },:class=>"btn btn-danger" do %> <i class="glyphicon glyphicon-trash"></i> <%= t(:link_delete) %>
				<% end %>
				<% end %> </td>
				<% end %>
			</tr>
			<% end %>
			<% else %>
			<tr>
				<td colspan="6"><%= t(:no_data)%></td>
			</tr>
			<% end %>
		</tbody>
	</table>
</div>
<div class="row">
	<div class="col-xs-6 col-sm-3 col-md-2">
		<% if can? :create, ComplimentCategory %>
		<%= link_to new_admin_compliment_category_path,:class=>"btn btn-primary" do %>
		<i class="glyphicon glyphicon-plus"></i> <%= t(:link_new) %>
		<% end %>
		<% end %>
	</div>
	<nav class="col-xs-12 col-sm-9 col-md-10">
		<%= paginate @admin_compliment_categories %>
	</nav>
</div>
