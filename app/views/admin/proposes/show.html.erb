<div class="sl_show">
	<p>
		<b><%=t(:title,scope:[:activerecord,:attributes,:propose]) %></b>
		<%= @admin_propose.title %>
	</p>
  <p>
		<b><%=t(:content,scope:[:activerecord,:attributes,:propose]) %></b>
		<%= simple_format(@admin_propose.content) %>
	</p>
	<p>
		<b><%=t(:enable,scope:[:activerecord,:attributes,:propose]) %></b>
		<%=raw flagToString(@admin_propose.enable) %>
	</p>
	<p>
  	<b><%=t(:label_created_at) %></b>
  	<%= @admin_propose.created_at %>
  </p>
  <p>
  	<b><%=t(:label_updated_at) %></b>
  	<%= @admin_propose.created_at %>
  </p>
</div>
<% if can? :update, @admin_propose %>
<%= link_to t(:link_edit), edit_admin_propose_path(@admin_propose) %> |
<% end %>
<%= link_to t(:link_list), admin_proposes_path %>
